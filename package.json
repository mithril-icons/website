{
  "name": "@mithril-icons/website",
  "version": "1.0.0",
  "description": "Demo website showcasing @mithril-icons packages",
  "scripts": {
    "start": "parcel src/index.html",
    "clean": "rimraf ./public .parcel-cache",
    "build": "npm run generate && parcel build --no-source-maps src/index.html",
    "rebuild": "npm run clean && npm run build",
    "generate": "ts-node -P tsconfig.json -O '{\"module\":\"commonjs\"}' src/_scripts/generate-readmes.ts",
    "test": "jest"
  },
  "main": "public/index.html",
  "keywords": [],
  "author": "Micha≈Ç Kijowski <michal@kijowski.dev> (https://kijowski.dev)",
  "license": "MIT",
  "browserslist": [
    "since 2017-06"
  ],
  "dependencies": {
    "mithril": "2.0.4"
  },
  "devDependencies": {
    "@mithril-icons/clarity": "1.1.1",
    "@mithril-icons/devicon": "1.0.0",
    "@mithril-icons/entypo": "1.0.0",
    "@mithril-icons/feather": "1.0.1",
    "@mithril-icons/font-awesome": "1.0.0",
    "@mithril-icons/jam": "1.0.0",
    "@mithril-icons/material-design": "1.0.0",
    "@mithril-icons/octicons": "1.0.0",
    "@types/jest": "25.1.2",
    "@types/marked": "0.7.2",
    "@types/mithril": "2.0.2",
    "@types/node": "13.7.0",
    "@typescript-eslint/eslint-plugin": "2.19.0",
    "@typescript-eslint/parser": "2.19.0",
    "autoprefixer": "9.7.4",
    "eslint": "6.8.0",
    "eslint-config-standard-with-typescript": "12.0.1",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "jest": "25.1.0",
    "marked": "0.8.0",
    "parcel": "2.0.0-alpha.3.2",
    "postcss-calc": "7.0.1",
    "postcss-import": "12.0.1",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "rimraf": "3.0.2",
    "ts-jest": "25.2.0",
