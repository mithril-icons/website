{
  "compilerOptions": {
    "target": "ES2018",
    "lib": ["DOM","DOM.Iterable","ES2018"],
    "module": "ESNext",
    "jsx": "react",
    "jsxFactory": "h",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "paths": {
      "~/*": [
        "./*"
      ]
    },
  },
  "include": ["src"],
}
